import java.util.*;
import java.io.*;

class Solution {
    public static String solution(int num) {
        String answer = "";
        if(num%2==0){
            answer = "Even";
        }else{
            answer= "Odd";
        }
        
        return answer;
    }
    
    public static void main(String[] args){
        Scanner sc= new Scanner(System.in);
        int N= sc.nextInt();
        System.out.println(solution(N));
    }
}
