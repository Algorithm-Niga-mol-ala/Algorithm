import java.util.*;
import java.io.*;

class Solution {
    public String solution(String s) {
        String answer = "";
        String[] str = s.split(" ");
        
        for(int i=0;i<str.length;i++){
            // String first = str[i].substring(0, 1);
           for(char x : str[i].toCharArray()){
                if(x.isUpperCase()){
                    x = x.toLowerCase();
                }
               System.out.println(x);
           } 
            
        }
        
        return answer;
    }
}
